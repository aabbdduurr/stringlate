{
  "name": "stringlate-backend",
  "version": "1.0.0",
  "description": "Backend for StringLate",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:dev": "nodemon",
    "docker:prune": "docker system prune -f",
    "docker:dev": "docker-compose -f docker-compose.dev.yml down && docker-compose -p stringlate-backend -f docker-compose.dev.yml up --build",
    "docker:prod": "docker-compose -f docker-compose.prod.yml down && docker-compose -p stringlate-backend -f docker-compose.prod.yml up --build",
    "test": "jest --config ./jest.config.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/supertest": "^6.0.2",
    "@types/jest": "^29.5.12",
    "express": "^4.19.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
